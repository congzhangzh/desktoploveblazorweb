    <OutputType>WinExe</OutputType>
    <StartupObject>BlazorBrowserNativeStyle.Program</StartupObject>
    <UserSecretsId>ae60eaf7-ac53-4469-b861-e493da2e3d6e</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
